{
  "extends": ["next/core-web-vitals", "plugin:storybook/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-restricted-imports": [
      "error",
      {
        "patterns": ["../*", "./*"]
      },
      {
        "endOfLine": "auto"
      }
    ]
  }
}
